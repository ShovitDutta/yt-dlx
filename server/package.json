{
  "name": "server",
  "version": "12.8.0",
  "license": "Apache-2.0",
  "scripts": {
    "start": "tsc && node build/index.js",
    "build": "tsc && pkg . --out-path package",
    "clean": "rimraf node_modules build package",
    "dev": "concurrently \"tsc --watch\" \"nodemon build/index.js\"",
    "build:win": "tsc && pkg . --out-path package --targets node18-win-x64",
    "build:mac": "tsc && pkg . --out-path package --targets node18-macos-x64",
    "build:linux": "tsc && pkg . --out-path package --targets node18-linux-x64",
    "postinstall": "node public/cprobe-win.js && node public/cprobe-mac.js && node public/cprobe-linux.js && shx chmod +x ."
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "axios": "^1.7.2",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "shx": "^0.3.4",
    "youtubei": "^1.4.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "concurrently": "^8.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.3",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "bin": "build/index.js",
  "pkg": {
    "scripts": [
      "build/index.js",
      "build/**/*.js"
    ],
    "assets": [
      "public/**/*",
      "public/icon.ico"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-linux-x64"
    ],
    "icon": "public/icon.ico"
  }
}
