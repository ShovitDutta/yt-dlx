{
    "expo": {
        "name": "yt-dlx",
        "slug": "yt-dlx",
        "scheme": "yt-dlx",
        "version": "1.0.0",
        "newArchEnabled": true,
        "orientation": "portrait",
        "ios": { "supportsTablet": true },
        "userInterfaceStyle": "automatic",
        "icon": "./assets/images/icon.png",
        "experiments": { "typedRoutes": true },
        "web": { "bundler": "metro", "output": "static", "favicon": "./assets/images/favicon.png" },
        "android": { "package": "com.shovit.ytdlx", "adaptiveIcon": { "foregroundImage": "./assets/images/adaptive-icon.png", "backgroundColor": "#ffffff" } },
        "plugins": ["expo-router", ["expo-splash-screen", { "image": "./assets/images/splash-icon.png", "imageWidth": 200, "resizeMode": "contain", "backgroundColor": "#ffffff" }]]
    }
}
